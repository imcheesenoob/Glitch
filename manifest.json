{
  "manifest_version": 3,
  "name": "Glitch Letters",
  "version": "1.0",
  "description": "Changes the font",
  "permissions": ["activeTab"],
  "icons": {
    "128": "https://www.i2symbol.com/images/text-symbols/a-symbol.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
