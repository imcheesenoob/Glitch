{
  "manifest_version": 2,
  "name": "Glitch Letters",
  "version": "1.0",
  "description": "Changes the font",
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
